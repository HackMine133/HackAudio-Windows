{
  "name": "hack-audio",
  "productName": "HackAudio",
  "version": "26.3.1",
  "main": "main.js",
  "description": "Audio Player",
  "author": "HackMine",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "jsmediatags": "^3.9.7"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.hackaudio.app",
    "productName": "HackAudio",
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "fileAssociations": [
      {
        "ext": ["mp3", "wav", "ogg", "flac", "m4a"],
        "name": "Audio File",
        "role": "Editor"
      }
    ]
  }
}